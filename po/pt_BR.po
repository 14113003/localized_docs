#. type: Content of: <html><body><div><ol><li>
#: orig-docs/Documentation.html:4387
msgid ""
"<tt>git clone git://phpmyadmin.git.sourceforge.net/gitroot/phpmyadmin/"
"phpmyadmin</tt>"
msgstr ""
"<tt>git clone git://phpmyadmin.git.sourceforge.net/gitroot/phpmyadmin/"
"phpmyadmin</tt>"

#. type: Content of: <html><body><div><ol><li>
#: orig-docs/Documentation.html:4390
msgid "add your stuff"
msgstr ""

#. type: Content of: <html><body><div><ol><li>
#: orig-docs/Documentation.html:4391
msgid "generate patch with your changes: <tt>git diff &gt; xxx.diff</tt>"
