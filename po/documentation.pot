msgstr ""

# 731af63575744c43982ba40d119edea0
#: ../source/faq.rst:1275
msgid "5.17 With Firefox, I cannot delete rows of data or drop a database."
msgstr ""

# a501145591914d40832a0e09188b84e9
#: ../source/faq.rst:1277
