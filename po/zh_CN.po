msgstr ""

#. type: Content of: <html><body><div><dl><dt>
#: ../phpmyadmin/Documentation.html:4277
msgid "<code>@VERBOSE@</code>"
msgstr ""

#. type: Content of: <html><body><div><dl><dd>
#: ../phpmyadmin/Documentation.html:4278
msgid ""
"Verbose MySQL server name as defined in <a href=\"#cfg_Servers_verbose"
"\">server configuration</a>"
msgstr ""

#. type: Content of: <html><body><div><dl><dt>
#: ../phpmyadmin/Documentation.html:4279
msgid "<code>@VSERVER@</code>"
msgstr ""

#. type: Content of: <html><body><div><dl><dd>
#: ../phpmyadmin/Documentation.html:4280
msgid "Verbose MySQL server name if set, otherwise normal"
msgstr ""

#. type: Content of: <html><body><div><dl><dt>
#: ../phpmyadmin/Documentation.html:4281
msgid "<code>@DATABASE@</code>"
msgstr ""

#. type: Content of: <html><body><div><dl><dd>
#: ../phpmyadmin/Documentation.html:4282
msgid "Currently opened database"
msgstr ""

#. type: Content of: <html><body><div><dl><dt>
#: ../phpmyadmin/Documentation.html:4283
msgid "<code>@TABLE@</code>"
msgstr ""

#. type: Content of: <html><body><div><dl><dd>
#: ../phpmyadmin/Documentation.html:4284
msgid "Currently opened table"
msgstr ""

#. type: Content of: <html><body><div><dl><dt>
#: ../phpmyadmin/Documentation.html:4285
#, fuzzy
